    "markdown-to-jsx": "6.11.4",
    "next": "9.5.3",
    "next-cookies": "2.0.3",
    "prop-types": "15.7.2",
    "rc-tooltip": "4.0.3",
    "react": "16.13.1",
    "react-code-blocks": "0.0.7",
    "react-dom": "16.13.1",
    "react-i18next": "11.4.0",
    "reactstrap": "8.4.1",
